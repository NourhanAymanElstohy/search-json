{
    "name": "nourayman/search-json",
    "description": "Search JSON columns with Arabic character normalization",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nourayman\\SearchJson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nourayman\\SearchJson\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nourayman\\SearchJson\\SearchJsonServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Nourhan Ayman",
            "email": "nourhanelstohy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
